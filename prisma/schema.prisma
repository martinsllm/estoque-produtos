// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id       String @id
  name     String
  price    Float
  quantity Int

  @@map("products")
}

model User {
  id       String @id
  name     String
  email    String @unique
  password String

  @@map("users")
}
